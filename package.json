{
  "name": "hp15c-calculator",
  "version": "1.0.0",
  "description": "HP15C-calculator application",
  "main": "main.js",
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder --publish never",
    "start": "electron ."
  },
  "build": {
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thesourorange/HP15C-calculator.git"
  },
  "keywords": [
    "Electron",
    "HP15C",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "sprintf": "^0.1.5",
    "jquery": "^3.4.1"
  },
  "devDependencies": {
    "electron": "^14.0.1",
    "electron-builder": "^22.11.7",
    "node": "^16.9.1"
  },
  "bugs": {
    "url": "https://github.com/thesourorange/HP15C-calculator/issues"
  },
  "homepage": "https://github.com/thesourorange/HP15C-calculator#readme"
}
